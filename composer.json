{
    "name": "metadocx/bloxs",
    "description": "Metadocx bloxs package.",
    "license": "MIT",
    "homepage": "https://metadocx.com",
    "support": {
        "issues": "https://github.com/metadocx/bloxs/issues",
        "source": "https://github.com/metadocx/bloxs"
    },
    "authors": [
        {
            "name": "Benoit Gauthier",
            "email": "bgauthier@metadocx.com"
        }
    ],    
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*"        
    },    
    "autoload": {
        "psr-4": {
            "Metadocx\\Bloxs\\": "/src"            
        }
    },
    "extra": {      
        "laravel": {
            "providers": [
                "Metadocx\\Bloxs\\MetadocxBloxsServiceProvider"
            ]
        }
    },    
    "config": {
        "sort-packages": true
    },    
    "minimum-stability": "dev",
    "require-dev": {
        "squizlabs/php_codesniffer": "4.0.x-dev"
    }
}
